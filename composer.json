{
    "name": "craffft/contao-logo",
    "description": "Logo module for Contao OpenSource CMS",
    "keywords": ["logo", "module"],
    "type": "contao-module",
    "homepage": "http://craffft.de",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Daniel Kiesel",
            "email": "daniel@craffft.de",
            "homepage": "http://craffft.de",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/Craffft/contao-logo/issues",
        "source": "https://github.com/Craffft/contao-logo"
    },
    "require": {
        "php": ">=5.6",
        "contao/core-bundle": "^3.2 || ^4.2",
        "contao-community-alliance/composer-plugin": "^2.4.0 || 3.*"
    },
    "autoload": {
        "classmap": ["system/"]
    },
    "replace": {
        "contao-legacy/logo": "*"
    },
    "extra": {
        "contao": {
            "sources": {
                "system/modules/logo": "system/modules/logo"
            }
        }
    }
}
